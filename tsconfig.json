{
  "compilerOptions": {
      "target": "ESNext",
      "lib": ["dom", "dom.iterable", "esnext"],
      "types": ["node", "vite/client", "vite-plugin-svgr/client"],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react-jsx",
      // "noImplicitAny": false,
      "noImplicitThis": false,
      "baseUrl": "./src",
      "paths": {
          "api": ["./api"],
          "api/*": ["./api/*"],
          "bpmn/custom": ["./bpmn/custom"],
          "bpmn/model": ["./bpmn/model"],
          "bpmn/translation": ["./bpmn/translation"],
          "components": ["./components"],
          "components/*": ["./components/*"],
          "img": ["./img"],
          "mock/*": ["./mock/*"],
          "models": ["./models"],
          "models/*": ["./models/*"],
          "pages": ["./pages"],
          "pages/*": ["./pages/*"],
          "redux/*": ["./redux/*"],
          "router": ["./router"],
          "utils/*": ["./utils/*"],
          "consts": ["./consts.ts"]
      }
  },
  "include": ["src"]
}